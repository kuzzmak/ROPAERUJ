		map.put("mikk", 1);
		map.put("jelo", 2);
		map.put("aaaa", 2);
		
		map = sortByValue(map);
		Map<String, Integer> map2 = sortByValue(map);
		for(Map.Entry<String, Integer> m: map2.entrySet()){
			System.out.println(m.getKey() + " " + m.getValue());
		}
		
